{
  "name": "astropass-nasa-space-apps",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "NASA Space Apps Challenge 2025 - Interactive astronaut training and space exploration experience",
  "author": "AstroPass Team",
  "license": "MIT",
  "keywords": [
    "nasa",
    "space-apps",
    "react",
    "astronaut-training",
    "space-exploration",
    "education"
  ],
  "homepage": "https://your-domain.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/astropass-nasa"
  },
  "scripts": {
    "dev": "vite --mode development",
    "build": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "build:analyze": "vite build --mode production && npx vite-bundle-analyzer dist/",
    "preview": "vite preview",
    "preview:prod": "vite build --mode production && vite preview",
    "lint": "eslint . --ext js,jsx,ts,tsx",
    "lint:fix": "eslint . --ext js,jsx,ts,tsx --fix",
    "type-check": "echo 'Type checking not configured - using PropTypes for runtime validation'",
    "test": "echo 'Tests not configured yet'",
    "clean": "rimraf dist",
    "start": "npm run dev",
    "serve": "npm run preview",
    "deploy:netlify": "npm run build && netlify deploy --prod --dir=dist",
    "deploy:vercel": "npm run build && vercel --prod",
    "health-check": "npm run lint && npm run build",
    "pre-commit": "npm run lint:fix && npm run build"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "@tanstack/react-query": "^5.83.0",
    "gl-matrix": "^3.4.4",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.32.0",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "eslint": "^9.32.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.15.0",
    "terser": "^5.44.0",
    "vite": "^7.1.9"
  }
}
